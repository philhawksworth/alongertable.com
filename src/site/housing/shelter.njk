---
pagination:
  data: shelters
  size: 1
  alias: place
permalink: housing/shelters/{{ place.name | slug }}/index.html
layout: default
title: Housing
---

<h2>{{ place.name }}</h2>
<ul>
  {% if place.phone %}<li>Phone: {{ place.phone }}</li>{% endif %}
  {% if place.address %}<li>Address: {{ place.address }}</li>{% endif %}
  {% if place.management %}<li>Management: {{ place.management }}</li>{% endif %}
  {% if place.url %}<li><a href="{{ place.url }}" target="_BLANK" rel="noopener">Website</a></li>{% endif %}
  {% if place.notes %}<div>

  {{ place.notes }}

  </div>{% endif %}
</ul>

